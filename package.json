    "scripts": {
        "cls": "npx -y rimraf node_modules out tmp package-lock.json yarn.lock",
        "upd": "yarn install && npx -y npm-check-updates -u && yarn install --force",
        "dev": "npx -y ts-node-dev src/application/Launcher.ts --dev",
        "prd": "node -r tsconfig-paths/register out/Launcher.js --prd",
        "cmp": "tsc --build",
        "dpl": "npm run upd && npm run cmp"
    },
